    "components": {
        "schemas": {
            "BasicResponse": {
                "properties": {
                    "code": {
                        "type": "integer",
                        "title": "Code"
                    },
                    "message": {
                        "type": "string",
                        "title": "Message"
                    },
                    "data": {
                        "anyOf": [
                            {
                                "items": {

                                },
                                "type": "array"
                            },
                            {

                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Data"
                    }
                },
                "type": "object",
                "required": [
                    "code",
                    "message",
                    "data"
                ],
                "title": "BasicResponse"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "RequestForChat": {
                "properties": {
                    "content": {
                        "type": "string",
                        "title": "Content",
                        "description": "用户的提问"
                    },
                    "user_name": {
                        "type": "string",
                        "title": "User Name",
                        "description": "用户名"
                    },
                    "session_id": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Session Id",
                        "description": "对话session_id, 获取对话上下文"
                    },
                    "task": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Task",
                        "description": "对话任务"
                    },
                    "tools": {
                        "anyOf": [
                            {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Tools",
                        "description": "可选的工具列表"
                    },
                    "image_content": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Image Content"
                    },
                    "file_content": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "File Content"
                    },
                    "url_content": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Url Content"
                    },
                    "audio_content": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Audio Content"
                    },
                    "video_content": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Video Content"
                    },
                    "vison_mode": {
                        "anyOf": [
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Vison Mode",
                        "default": false
                    },
                    "deepsearch_mode": {
                        "anyOf": [
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Deepsearch Mode",
                        "default": false
                    },
                    "sql_interpreter": {
                        "anyOf": [
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Sql Interpreter",
                        "default": false
                    },
                    "code_interpreter": {
                        "anyOf": [
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Code Interpreter",
                        "default": false
                    }
                },
                "type": "object",
                "required": [
                    "content",
                    "user_name"
                ],
                "title": "RequestForChat"
            },
            "RequestForUserChangeInfo": {
                "properties": {
                    "user_name": {
                        "type": "string",
                        "title": "User Name"
                    },
                    "user_role": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "User Role"
                    },
                    "status": {
                        "anyOf": [
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Status"
                    }
                },
                "type": "object",
                "required": [
                    "user_name",
                    "user_role",
                    "status"
                ],
                "title": "RequestForUserChangeInfo"
            },
            "RequestForUserLogin": {
                "properties": {
                    "user_name": {
                        "type": "string",
                        "title": "User Name"
                    },
                    "password": {
                        "type": "string",
                        "title": "Password"
                    }
                },
                "type": "object",
                "required": [
                    "user_name",
                    "password"
                ],
                "title": "RequestForUserLogin"
            },
            "RequestForUserRegister": {
                "properties": {
                    "user_name": {
                        "type": "string",
                        "title": "User Name"
                    },
                    "password": {
                        "type": "string",
                        "title": "Password"
                    },
                    "user_role": {
                        "type": "string",
                        "title": "User Role"
                    }
                },
                "type": "object",
                "required": [
